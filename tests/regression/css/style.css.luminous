  <TYPE>border-top</TYPE>:<NUMERIC>1px</NUMERIC> <VALUE>solid</VALUE> <NUMERIC>#B5CFFF</NUMERIC>;
}

<VARIABLE>.header</VARIABLE>
{
  <TYPE>color</TYPE>:<VALUE>black</VALUE>;
  <TYPE>padding-left</TYPE>:<NUMERIC>3em</NUMERIC>;
  <TYPE>padding-top</TYPE>:<NUMERIC>2px</NUMERIC>;
  <TYPE>background-image</TYPE>:<FUNCTION>url</FUNCTION>(/<VALUE>img</VALUE>/<VALUE>headerblend2</VALUE>.<VALUE>png</VALUE>);
  <TYPE>background-repeat</TYPE>:<VALUE>repeat-x</VALUE>;
